{
  "name": "bmi-meter",
  "description": "Yet another BMI calculator",
  "version": "1.1.1",
  "private": false,
  "author": "Yan <i@yinyan.fr>",
  "scripts": {
    "dev": "umi dev",
    "build": "umi build",
    "postinstall": "umi setup",
    "setup": "umi setup",
    "start": "npm run dev",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:prettier": "prettier -c --write \"src/**/*.(js|jsx|ts|tsx|less|css|md|json)\" --end-of-line auto",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/pro-components": "^2.4.6",
    "@umijs/plugins": "^4.0.67",
    "ahooks": "^3.7.7",
    "antd": "^5.4.6",
    "react-darkreader": "^1.5.6",
    "umi": "^4.0.67"
  },
  "devDependencies": {
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "firebase": "^9.21.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.3"
  }
}